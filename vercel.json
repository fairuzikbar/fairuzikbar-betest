{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/betest": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
